<p id="notice"><%= notice %></p>

<p>
  <strong>Description:</strong>
  <%= @task.description %>
</p>

<p>
  <strong>Complete:</strong>
  <% if @task.complete %>
    <%= 'Task Complete' %>
  <% else %>
    <%= 'Task Incomplete' %>
  <% end %>
</p>

<p>
  <strong>User:</strong>
  <%= @task.user.name %>
</p>

<%= link_to 'Edit', edit_user_task_path(@task.user, @task) %> |
<%= link_to 'Back', user_tasks_path %>
